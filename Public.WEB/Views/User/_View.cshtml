@model Public.WEB.Models.User.UserResponse

<div class="modal-body">
    <dl class="row">
        <dt class="col-sm-4">First Name</dt>
        <dd class="col-sm-8">@Model.FirstName</dd>

        <dt class="col-sm-4">Last Name</dt>
        <dd class="col-sm-8">@Model.LastName</dd>

        <dt class="col-sm-4">Email</dt>
        <dd class="col-sm-8">@Model.Email</dd>

        <dt class="col-sm-4">Phone</dt>
        <dd class="col-sm-8">@Model.PhoneNumber</dd>

        <dt class="col-sm-4">Employment Type</dt>
        <dd class="col-sm-8">@Model.EmploymentType</dd>

        <dt class="col-sm-4">User Type</dt>
        <dd class="col-sm-8">@Model.UserType</dd>

        <dt class="col-sm-4">Created At</dt>
        <dd class="col-sm-8">@Model.CreatedAt.ToString("g")</dd>
    </dl>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
</div>
